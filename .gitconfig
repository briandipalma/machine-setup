[alias]
        a = add
        ap = add -p
        b = branch
        c = commit -v
        caf = "!git commit --amend --no-edit; git push --force-with-lease"
        o = checkout
        d = diff
        fp = fetch -p
        l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev --date=relative
        pl = pull
        pu = push
        puf = push --force-with-lease
        rbm = rebase master
        rbc = rebase --continue
        s = status
[push]
        default = simple
[user]
        name = Brian Di Palma
        email = briandipalma [at] outlook.com
